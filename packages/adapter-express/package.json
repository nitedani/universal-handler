    "@universal-middleware/tests": "workspace:*",
    "express": "catalog:",
    "helmet": "^7.1.0",
    "rimraf": "^6.0.0",
    "tsup": "^8.2.4",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
