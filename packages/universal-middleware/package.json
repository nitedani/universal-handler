{
  "name": "universal-middleware",
  "type": "module",
  "version": "0.3.5",
  "description": "",
  "license": "MIT",
  "repository": "https://github.com/magne4000/universal-handler",
  "bugs": {
    "url": "https://github.com/magne4000/universal-handler/issues"
  },
  "keywords": [
    "esbuild",
    "vite",
    "rollup",
    "tsup",
    "transform",
    "universal-middleware",
    "middleware",
    "handlers",
    "server-adapters",
    "express-middleware",
    "hono-middleware",
    "hattip-middleware",
    "webroute-middleware",
    "cloudflare-middleware",
    "fastify-middleware",
    "h3-middleware",
    "universal-server",
    "context-middleware",
    "request-handling"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./vite": {
      "types": "./dist/rollup.d.ts",
      "import": "./dist/rollup.js",
      "default": "./dist/rollup.js"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "import": "./dist/rollup.js",
      "default": "./dist/rollup.js"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "import": "./dist/esbuild.js",
      "default": "./dist/esbuild.js"
    }
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsup",
    "prepack": "pnpm build",
    "test:typecheck": "tsc -p tsconfig.json --noEmit",
    "release": "LANG=en_US release-me patch",
    "release:minor": "LANG=en_US release-me minor",
    "release:commit": "LANG=en_US release-me commit",
    "test": "vitest run"
  },
  "dependencies": {
    "@universal-middleware/cloudflare": "workspace:*",
    "@universal-middleware/core": "workspace:*",
    "@universal-middleware/express": "workspace:*",
    "@universal-middleware/fastify": "workspace:*",
    "@universal-middleware/hattip": "workspace:*",
    "@universal-middleware/hono": "workspace:*",
    "@universal-middleware/webroute": "workspace:*",
    "oxc-transform": "^0.29.0",
    "package-up": "^5.0.0",
    "unplugin": "^1.14.1"
  },
  "devDependencies": {
    "@brillout/release-me": "catalog:",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@swc/core": "catalog:",
    "esbuild": "^0.23.1",
    "rimraf": "catalog:",
    "rollup": "^4.21.3",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "@rollup/plugin-node-resolve": "^15",
    "esbuild": "^0.23.1",
    "rollup": "^4",
    "vite": ">=5"
  },
  "peerDependenciesMeta": {
    "rollup": {
      "optional": true
    },
    "@rollup/plugin-node-resolve": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    }
  }
}
